   pipeline{
      agent any
      stages {
         stage('Build'){
            steps {
             echo "Build The Project"
           }
            }
         stage('deploy'){
            steps {
             echo "Deploy The Project"
            }   
             }
         stage('Test'){
            steps {
              echo "Test The Project"
            }
              }
         stage('Delivery'){
            steps {
              echo "Delivery The Project"
              }   
           }
        }
     } 
}
      
